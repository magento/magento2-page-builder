{
  "name": "@gene/bluefoot",
  "version": "1.0.0",
  "description": "Bluefoot Node utilities, at this point primarily for testing",
  "scripts": {
    "start": "npm test -S -- --no-single-run",
    "test": "karma start view/adminhtml/test/karma.conf.js",
    "debug": "cross-env KARMA_DEBUG=1 npm test -S -- --log-level debug --no-single-run --browsers",
    "test:allbrowsers": "npm test -S -- --browsers Chrome,Firefox,Safari,MobileSafari",
    "start:allbrowsers": "npm run -S test:allbrowsers -- --no-single-run"
  },
  "devDependencies": {
    "@types/jquery": "^3.2.10",
    "@types/knockout": "^3.4.43",
    "@types/magento2": "https://github.com/DaveMacaulay/types-magento2.git",
    "@types/requirejs": "^2.1.30",
    "@types/underscore": "^1.8.1",
    "babel-core": "6.26.0",
    "babel-plugin-magento2": "https://github.com/DaveMacaulay/babel-plugin-magento2.git",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-system-import-transformer": "^3.1.0",
    "babel-plugin-transform-es2015-modules-amd": "^6.24.1",
    "babel-plugin-transform-es2015-modules-simple-amd": "^0.3.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^3.1.4",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-typescript": "3",
    "gulp-typescript-babel": "^1.0.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ios-simulator-launcher": "^0.0.4",
    "karma-jasmine": "^1.1.0",
    "karma-jquery": "^0.1.0",
    "karma-mocha-reporter": "^2.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-requirejs": "^1.1.0",
    "karma-safari-launcher": "^1.0.0",
    "requirejs": "^2.3.2",
    "typescript": "^2.4.2"
