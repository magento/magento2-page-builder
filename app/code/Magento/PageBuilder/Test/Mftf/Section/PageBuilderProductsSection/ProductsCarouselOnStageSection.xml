        <element name="advancedConfigurationPath" type="text" parameterized="true" selector="(//div[contains(@class,&quot;pagebuilder-products&quot;)]//div[contains(@data-appearance,&quot;carousel&quot;)])[{{indexOfProductGroup}}]"/>
        <element name="alignment" type="text" parameterized="true" selector="(//div[contains(@class,'pagebuilder-products')]//div[contains(@data-appearance,'carousel')])[{{indexOfProductGroup}}][contains(@style,'text-align: {{alignment}};')]" deprecated="Styles are not inline"/>
        <element name="noAlignment" type="text" parameterized="true" selector="(//div[contains(@class,'pagebuilder-products')]//div[contains(@data-appearance,'carousel')])[{{indexOfProductGroup}}][not(contains(@style,'text-align:'))]" deprecated="Styles are not inline"/>
        <element name="border" type="text" parameterized="true" selector="(//div[contains(@class,'pagebuilder-products')]//div[contains(@data-appearance,'carousel')])[{{indexOfProductGroup}}][contains(@style,'border-style: {{borderStyle}};')]" deprecated="Styles are not inline"/>
        <element name="borderColor" type="text" parameterized="true" selector="(//div[contains(@class,'pagebuilder-products')]//div[contains(@data-appearance,'carousel')])[{{indexOfProductGroup}}][contains(@style,'border-color: {{borderColor}};')]" deprecated="Styles are not inline"/>
        <element name="noBorderColor" type="text" parameterized="true" selector="(//div[contains(@class,'pagebuilder-products')]//div[contains(@data-appearance,'carousel')])[{{indexOfProductGroup}}][not(contains(@style,'border-color:'))]" deprecated="Styles are not inline"/>
        <element name="borderWidth" type="text" parameterized="true" selector="(//div[contains(@class,'pagebuilder-products')]//div[contains(@data-appearance,'carousel')])[{{indexOfProductGroup}}][contains(@style,'border-width: {{borderWidth}}px;')]" deprecated="Styles are not inline"/>
        <element name="borderRadius" type="text" parameterized="true" selector="(//div[contains(@class,'pagebuilder-products')]//div[contains(@data-appearance,'carousel')])[{{indexOfProductGroup}}][contains(@style,'border-radius: {{borderRadius}}px;')]" deprecated="Styles are not inline"/>
        <element name="cssClasses" type="text" parameterized="true" selector="(//div[contains(@class,'pagebuilder-products')]//div[contains(@data-appearance,'carousel')])[{{indexOfProductGroup}}][contains(@class,'{{cssClass}}')]"/>
        <element name="noCssClasses" type="text" parameterized="true" selector="(//div[contains(@class,'pagebuilder-products')]//div[contains(@data-appearance,'carousel')])[{{indexOfProductGroup}}][not(@class)]"/>
        <element name="margins" type="text" parameterized="true" selector="(//div[contains(@class,'pagebuilder-products')]//div[contains(@data-appearance,'carousel')])[{{indexOfProductGroup}}][contains(@style,'margin: {{margin}}px;')]" deprecated="Styles are not inline"/>
        <element name="padding" type="text" parameterized="true" selector="(//div[contains(@class,'pagebuilder-products')]//div[contains(@data-appearance,'carousel')])[{{indexOfProductGroup}}][contains(@style,'padding: {{padding}}px;')]" deprecated="Styles are not inline"/>
