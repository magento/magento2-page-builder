<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="urn:magento:mftf:Page/etc/SectionObject.xsd">
    <section name="SlideOnFrontendEmptyLink">
        <element name="linkUrl" type="button" selector="(//div[@data-content-type='slide'])[{{slideIndex}}]/div[@data-element='empty_link'][not(@href) and not(@target)][not(@href='{{link}}')]" parameterized="true"/>
