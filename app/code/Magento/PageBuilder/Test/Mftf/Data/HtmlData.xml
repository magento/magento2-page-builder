        <data key="value">&lt;style type=&quot;text/css&quot;&gt;.css__specificity { text-align: center; }&lt;/style&gt;</data>
    </entity>
    <entity name="PageBuilderHtmlPropertyCssSpecificityWithBodyId" type="pagebuilder_html_property">
