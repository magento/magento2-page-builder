<!-- Whitelist words found in 3rd party libraries -->
<config>
    <whitelist>
        <item>
            <component type="module" name="Magento_PageBuilder" />
            <path>view/base/web/js/resource/highlight/CHANGES.md</path>
            <word>twig</word>
        </item>
        <item>
            <component type="module" name="Magento_PageBuilder" />
            <path>view/base/web/js/resource/highlight/highlight.pack.js</path>
            <word>Enterprise</word>
        </item>
    </whitelist>
</config>
