<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/magento2-functional-testing-framework/src/Magento/FunctionalTestingFramework/DataGenerator/etc/dataProfileSchema.xsd">
    <entity name="PageBuilderImagePropertyJpg" type="pagebuilder_image_property">
        <data key="name">Image</data>
        <data key="section">general</data>
        <data key="fieldName">image</data>
        <data key="value">magento_black_vertical.jpg</data>
        <data key="fileType">jpg</data>
        <data key="path">media/wysiwyg/bluefoot/m/a/magento_black_vertical</data>
    </entity>
    <entity name="PageBuilderMobileImagePropertyGif" type="pagebuilder_mobile_image_property">
        <data key="name">Mobile Image</data>
        <data key="section">general</data>
        <data key="fieldName">mobile_image</data>
        <data key="value">m-logo.gif</data>
        <data key="fileType">gif</data>
        <data key="path">media/wysiwyg/bluefoot/m/-/m-logo</data>
    </entity>
    <entity name="PageBuilderImageAltTextProperty" type="pagebuilder_alt_text_property">
        <data key="name">Alternative Text</data>
        <data key="section">seo</data>
        <data key="fieldName">alt</data>
        <data key="value">This is alternative text.</data>
    </entity>
    <entity name="PageBuilderImageTitleTagProperty" type="pagebuilder_title_tag_property">
        <data key="name">Title Tag</data>
        <data key="section">seo</data>
        <data key="fieldName">title_tag</data>
        <data key="value">This is a Title Tag</data>
    </entity>
    <entity name="PageBuilderImageLightboxProperty" type="pagebuilder_lightbox_property">
        <data key="name">Has Lightbox?</data>
        <data key="section">extra</data>
        <data key="fieldName">lightbox</data>
        <data key="value">Yes</data>
    </entity>
    <entity name="PageBuilderImageShowCaptionProperty" type="pagebuilder_title_tag_property">
        <data key="name">Show Caption</data>
        <data key="section">extra</data>
        <data key="fieldName">show_caption</data>
        <data key="value">Yes</data>
    </entity>
</config>
